<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:WPF_TravelApp"                    
                    xmlns:v="clr-namespace:WPF_TravelApp.Views"
                    xmlns:vm="clr-namespace:WPF_TravelApp.ViewModels">
    <DataTemplate DataType="{x:Type vm:HelloViewModel}">
        <v:HelloView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:SignUpViewModel}">
        <v:SignUpView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:TripAddViewModel}">
        <v:TripAddView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:TripDbAddViewModel}">
        <v:TripDbAddView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:HomePageViewModel}">
        <v:HomePageView/>
    </DataTemplate>
</ResourceDictionary>