<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:WPF_TravelApp"                    
                    xmlns:v="clr-namespace:WPF_TravelApp.Views"
                    xmlns:vm="clr-namespace:WPF_TravelApp.ViewModels">
    <DataTemplate DataType="{x:Type vm:LoginViewModel}">
        <v:LoginView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:SignUpViewModel}">
        <v:SignUpView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:MainViewModel}">
        <v:MainView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:HomePageViewModel}">
        <v:HomePageView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:AddTripViewModel}">
        <v:AddTripView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:CheckBoxViewModel}">
        <v:CheckBoxView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:TicketViewModel}">
        <v:TicketView/>
    </DataTemplate>
</ResourceDictionary>