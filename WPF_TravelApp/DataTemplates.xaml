<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:WPF_TravelApp"                    
                    xmlns:v="clr-namespace:WPF_TravelApp.Views"
                    xmlns:vm="clr-namespace:WPF_TravelApp.ViewModels">
    <DataTemplate DataType="{x:Type vm:LoginViewModel}">
        <v:LoginView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:SignUpViewModel}">
        <v:SignUpView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:TripObserveUCViewModel}">
        <v:TripObserveUCView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:TestWindowViewModel}">
        <v:TestWindowView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:MainViewModel}">
        <v:MainView/>
    </DataTemplate>
</ResourceDictionary>